{
  "name": "labhealth",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "setup": "npm i && npm run server && npm run client && npm run migrate",
    "server": "cd server && npm i",
    "client": "cd client && npm i",
    "migrate": "cd client && npx truffle migrate",
    "serverStart": "cd server && npm start",
    "clientStart": "cd client && npm start",
    "start": "concurrently \"npm run serverStart\" \"npm run clientStart\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
